public class HarmonikOrtlama {
    public static void main(String[] args) {
        int[] number = {1, 2, 3, 4, 5};
        double sum = 0.0;
        for (int i = 0; i < number.length; i++) {
            sum += (1.0/i);
        }
        double avarage = number.length / sum ;

        System.out.println(avarage);
    }
}
